+    private function requestUsingFopen($url)
+    {
+        $data = '';
+        $connection = @fopen($url, 'rb');
+
+        if (!$connection) {
+            $this->setError(self::ERROR_FOPEN_FAIL);
+            return false;
+        }
+
+        while (!@feof($connection)) {
+            $data .= @fgets($connection, 4096);
+        }
+
+        @fclose($connection);
+
+        return $data;
+    }
