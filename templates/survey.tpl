{block name=head}<style type="text/css"></style>{/block}{block name=body}<div id="pagehead">	<div class="bannerout">      <img src="{if $listing_image}{$listing_image}{else}/images/newsdet-banner.jpg{/if}" alt="{$listing_name} banner" />	</div></div><div id="surveycont"><form action="/process/survey" method="POST">  <div class="container">    <div class="row">	     	<div class="col-md-8 col-md-offset-2 text-center">	 	 		<h1>Feedback - recent purchase</h1>	 	 		<input type="hidden" name="formToken" id="formToken" value="{$token}" />	 	 		<input type="hidden" value="Survey" name="form_name" id="form_name" />	 	 		<input type="hidden" name="timestamp" id="timestamp" value="{$timestamp}" />                <input type="hidden" name="surveytoken" id="surveytoken" value="{$surveytoken}" />	 	 		<input type="hidden" name="uid" value="{$user.id}">	 	 	</div>      		<div class="col-md-8 col-md-offset-2 text-center">              <p class="bold">Thinking about your recent purchase through the MedicAlert Foundation. How satisfied were you with the:</p>            </div>            {foreach from=$questions[1] key=k item=qset1}              <div class="col-md-8 col-md-offset-2 form-group {if $qset1[2] eq '0' || $qset1[2] eq '2'}text-center questiondiv{/if} {if $qset1[2] eq '0'}childdiv{/if}" {if $qset1[2] eq '0'}id="childdiv1"{/if}>                <p class="bold">{$qset1[1]}</p>                <input type="hidden" name="questionid[{$qset1[0]}]" value="{$qset1[0]}">                {if $qset1[2] eq '0'}                    <textarea class="form-control" name="text[{$qset1[0]}]"></textarea>                {else}                  {foreach from=$answers[$qset1[2]] key=ak item=aset}                    <p><input class="form-control" type="radio" name="answer[{$qset1[0]}]" id="answer-{$qset1[0]}-{$ak}" value="{$aset}" {if $qset1[2] eq '2'}data-childdiv="childdiv1"{/if}><label for="answer-{$qset1[0]}-{$ak}">{$aset}</label></p>                  {/foreach}                {/if}              </div>            {/foreach}          <div style="height:0;overflow:hidden;">            <input value="" type="text" name="honeypot" id="honeypot" tabindex="-1" autocomplete="off">          </div>    </div>  </div>  <div class="container">    <div class="row">      <div class="col-md-8 col-md-offset-2 text-center">        <h1><b>Feedback - completed user profile</b></h1>      </div>            <div class="col-md-8 col-md-offset-2 text-center">              <b>Thinking about the process of creating your Medicalert profile and finalising your membership. How satisfied were you with the:</b>            </div>            {foreach from=$questions[2] key=k item=qset1}              <div class="col-md-8 col-md-offset-2 {if $qset1[2] eq '0' || $qset1[2] eq '2'}text-center questiondiv{/if} {if $qset1[2] eq '0'}childdiv{/if}" {if $qset1[2] eq '0'}id="childdiv2"{/if}>                <p><b>{$qset1[1]}</b></p>                <input type="hidden" name="questionid[{$qset1[0]}]" value="{$qset1[0]}">                {if $qset1[2] eq '0'}                    <textarea name="text[{$qset1[0]}]"></textarea>                {else}                  {foreach from=$answers[$qset1[2]] key=ak item=aset}                    <p><input type="radio" name="answer[{$qset1[0]}]" id="answer-{$qset1[0]}-{$ak}" value="{$aset}" {if $qset1[2] eq '2'}data-childdiv="childdiv2"{/if}><label for="answer-{$qset1[0]}-{$ak}">{$aset}</label></p>                  {/foreach}                {/if}              </div>            {/foreach}      <div class="col-md-8 col-md-offset-2 text-center">        <p><input type="checkbox" name="feedbackCheck"> I would like to be contacted about my feedback.</p>        <input type="submit" value"SUBMIT" class="btn-red btn">      </div>    </div>  </div></form></div>{/block}{* Place additional javascript here so that it runs after General JS includes *}{block name=tail}<script type="text/javascript">	$( document ).ready(function(){	  //$(".childdiv").hide();	  $('.questiondiv input[type="radio"]').click(function() {           if($(this).attr('value') == 'Yes') {                var divId = $(this).data('childdiv');                $("#"+divId).show();           }else{             var divId = $(this).data('childdiv');             $("#"+divId).hide();           }       });	});</script>{/block}